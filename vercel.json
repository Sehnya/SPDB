{
  "version": 2,
  "buildCommand": "bun install && bun run build.ts",
  "outputDirectory": "dist",
  "devCommand": "bun dev",
  "functions": {
    "api/**/*.{js,ts}": {
      "runtime": "@vercel/node@5.3.13"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
